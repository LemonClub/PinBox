<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PinSelectButton">
        <attr name="text" format="reference|string" />
        <attr name="active" format="reference|boolean" />

        <attr name="text_color_normal" format="reference|color" />
        <attr name="text_color_select" format="reference|color" />
    </declare-styleable>

    <declare-styleable name="PinHeader">
        <attr name="pin_left_text" format="reference|string" />
        <attr name="pin_left_color" format="reference|color" />
        <attr name="pin_left_active" format="reference|boolean" />

        <attr name="pin_right_text" format="reference|string" />
        <attr name="pin_right_color" format="reference|color" />
        <attr name="pin_right_active" format="reference|boolean" />

        <attr name="pin_underline" format="reference|boolean" />
    </declare-styleable>
</resources>